// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" // หรือฐานข้อมูลที่คุณใช้งาน
  url      = env("DATABASE_URL")
}

model upload_file {
  id           Int      @id @default(autoincrement())
  file_name    String
  file_hash    String
  file_size    Int
  created_date DateTime @default(now())
}
